	states.swim = not states.swim
	buttons.swim.Text = "üèä Swim in Air: " .. (states.swim and "ON" or "OFF")
	
	if states.swim then
		activateSwim()
	else
		if plr.Character and plr.Character:FindFirstChild("Humanoid") then
			local humanoid = plr.Character.Humanoid
			humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, true)
			humanoid:SetStateEnabled(Enum.HumanoidStateType.Running, true)
		end
	end
end)

-- AURAKILL
buttons.auraKill = createButton("‚ò†Ô∏è AuraKill: OFF")
buttons.auraKill.Position = UDim2.new(0.04, 0, 0, yOffset)
