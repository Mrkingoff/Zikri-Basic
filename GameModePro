		warn("[AURAKILL] You need to equip a tool first!")
		return
	end
	
	states.auraKill = not states.auraKill
	buttons.auraKill.Text = "‚ò†Ô∏è AuraKill: " .. (states.auraKill and "ON" or "OFF")
	
	if states.auraKill then
		activateAuraKill()
	end
end)

