end)

-- INVISIBLE MODE
buttons.invisible = createButton("üëª Invisible: OFF")
buttons.invisible.Position = UDim2.new(0.04, 0, 0, yOffset)
yOffset = yOffset + 47

buttons.invisible.MouseButton1Click:Connect(function()
	states.invisible = not states.invisible
	buttons.invisible.Text = "üëª Invisible: " .. (states.invisible and "ON" or "OFF")
	
	toggleInvisible()
	print("[INVISIBLE] Mode: " .. (states.invisible and "ON" or "OFF"))
end)

-- RESET CHARACTER
buttons.reset = createButton("üîÑ Reset Character")
buttons.reset.Position = UDim2.new(0.04, 0, 0, yOffset)
yOffset = yOffset + 47

buttons.reset.MouseButton1Click:Connect(function()
	if plr.Character and plr.Character:FindFirstChild("Humanoid") then
		plr.Character.Humanoid.Health = 0
	end
end)

-- CLEANUP ON GAME CLOSE
plr.CharacterRemoving:Connect(function()
	for _, conn in pairs(connections) do
		if typeof(conn) == "RBXScriptConnection" then
			conn:Disconnect()
		end
	end
	connections = {}
	
	-- Cleanup aura
	for _, part in pairs(auraKillParts) do
		if part then part:Destroy() end
	end
	auraKillParts = {}
end)

--=== STYLING ===--
-- Rounded corners for frame
local frameCorner = Instance.new("UICorner")
frameCorner.CornerRadius = UDim.new(0, 12)
frameCorner.Parent = frame

local topbarCorner = Instance.new("UICorner")
topbarCorner.CornerRadius = UDim.new(0, 12, 0, 0)
topbarCorner.Parent = topbar

-- Drop shadow effect
local shadow = Instance.new("ImageLabel")
shadow.Name = "Shadow"
shadow.Size = UDim2.new(1, 10, 1, 10)
shadow.Position = UDim2.new(0, -5, 0, -5)
shadow.BackgroundTransparency = 1
shadow.Image = "rbxassetid://5554236805"
shadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
shadow.ImageTransparency = 0.8
shadow.ScaleType = Enum.ScaleType.Slice
shadow.SliceCenter = Rect.new(10, 10, 118, 118)
shadow.Parent = frame

-- Make shadow render behind frame
shadow.ZIndex = frame.ZIndex - 1

