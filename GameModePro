--=== FUNCTIONS ===--
local function createButton(text)
	local btn = Instance.new("TextButton")
	btn.Name = text
	btn.Size = UDim2.new(0.92, 0, 0, 42)
	btn.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
	btn.TextColor3 = Color3.fromRGB(255, 255, 255)
	btn.Text = text
	btn.TextSize = 16
	btn.Font = Enum.Font.Gotham
	btn.AutoButtonColor = true
	btn.Parent = scroll
	
	local corner = Instance.new("UICorner")
	corner.CornerRadius = UDim.new(0, 8)
	corner.Parent = btn
	
	return btn
end

local function updateFramePosition()
	if frame.Visible then
		frame.Position = UDim2.new(
			toggle.Position.X.Scale,
			toggle.Position.X.Offset + 70,
			toggle.Position.Y.Scale,
			toggle.Position.Y.Offset - 50
		)
	end
end

--=== TOGGLE LOGIC ===--
toggle.MouseButton1Click:Connect(function()
	frame.Visible = not frame.Visible
	updateFramePosition()
end)

closeBtn.MouseButton1Click:Connect(function()
	frame.Visible = false
end)

game:GetService("RunService").RenderStepped:Connect(updateFramePosition)

--=== VARIABLES ===--
local states = {
	god = false,
	extraLife = false,
	antiBlock = false,
	antiNpc = false,
	ws = false,
	esp = false,
	noclip = false,
	prompt = false,
	float = false,
	swim = false,
	auraKill = false,
	invisible = false,
	clickTP = false
}

local connections = {}
local auraKillParts = {}
local extraLifeUsed = false
local originalTransparency = {}

--=== SAFETY SYSTEM: EXTRA LIFE ===--
local function setupExtraLife()
	if not plr.Character then return end
	
	local humanoid = plr.Character:FindFirstChild("Humanoid")
	if not humanoid then return end
	
	humanoid.Died:Connect(function()
		if states.extraLife and not extraLifeUsed then
			extraLifeUsed = true
			task.wait(0.5)
			
			if plr.Character then
				local newHumanoid = plr.Character:FindFirstChild("Humanoid")
				if newHumanoid then
					-- Set to critical state (1 health)
					newHumanoid.Health = 1
					newHumanoid.MaxHealth = 100
					
					-- Add visual effect
					local effect = Instance.new("ParticleEmitter")
					effect.Color = ColorSequence.new(Color3.fromRGB(255, 0, 0))
					effect.Size = NumberSequence.new(0.5)
					effect.Lifetime = NumberRange.new(1)
					effect.Rate = 50
					effect.Speed = NumberRange.new(5)
					effect.Parent = plr.Character:FindFirstChild("HumanoidRootPart") or plr.Character:FindFirstChild("Head")
					
					task.delay(2, function()
						effect:Destroy()
					end)
					
					print("[SAFETY] Extra life activated! Health: 1")
				end
			end
			
			-- Reset after 30 seconds
			task.delay(30, function()
				extraLifeUsed = false
			end)
		end
	end)
end

-- Setup extra life on character added
plr.CharacterAdded:Connect(function()
	setupExtraLife()
end)

if plr.Character then
	setupExtraLife()
end

--=== AURA KILL SYSTEM ===--
local function activateAuraKill()
	if not plr.Character then return end
	
	local character = plr.Character
	local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
	if not humanoidRootPart then return end
	
	-- Destroy previous aura parts
	for _, part in pairs(auraKillParts) do
		if part then part:Destroy() end
	end
	auraKillParts = {}
	
	-- Create aura sphere
	local aura = Instance.new("Part")
	aura.Name = "AuraKillSphere"
	aura.Shape = Enum.PartType.Ball
	aura.Size = Vector3.new(10, 10, 10)
	aura.Transparency = 0.7
	aura.Color = Color3.fromRGB(255, 0, 0)
	aura.Material = EnumMaterial.Neon
	aura.CanCollide = false
	aura.Anchored = true
	aura.Parent = workspace
	
	local weld = Instance.new("Weld")
	weld.Part0 = humanoidRootPart
	weld.Part1 = aura
	weld.C0 = CFrame.new(0, 0, 0)
	weld.Parent = aura
	
	table.insert(auraKillParts, aura)
	
	-- Damage loop
	task.spawn(function()
		while states.auraKill do
			task.wait(0.5)
			
			-- Check if holding tool
			local tool = character:FindFirstChildOfClass("Tool")
			if not tool then
				states.auraKill = false
				buttons.auraKill.Text = "‚ò†Ô∏è AuraKill: OFF"
				break
			end
			
			-- Damage all nearby players
			for _, otherPlayer in pairs(game:GetService("Players"):GetPlayers()) do
				if otherPlayer ~= plr and otherPlayer.Character then
					local targetHRP = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
					if targetHRP then
						local distance = (humanoidRootPart.Position - targetHRP.Position).Magnitude
						if distance <= 20 then -- 20 stud radius
							local humanoid = otherPlayer.Character:FindFirstChild("Humanoid")
							if humanoid then
								humanoid:TakeDamage(100) -- Instant kill
								
								-- Visual effect
								local hitEffect = Instance.new("Part")
								hitEffect.Size = Vector3.new(2, 2, 2)
								hitEffect.Position = targetHRP.Position
								hitEffect.Color = Color3.fromRGB(255, 0, 0)
								hitEffect.Material = EnumMaterial.Neon
								hitEffect.Transparency = 0.5
								hitEffect.Anchored = true
								hitEffect.CanCollide = false
								hitEffect.Parent = workspace
								
								task.delay(0.5, function()
									hitEffect:Destroy()
								end)
							end
						end
					end
				end
			end
		end
		
		-- Cleanup aura
		for _, part in pairs(auraKillParts) do
			if part then part:Destroy() end
		end
		auraKillParts = {}
	end)
end

--=== IMPROVED FLOAT MODE ===--
local function activateFloat()
	task.spawn(function()
		while states.float do
			task.wait(0.05)
			if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
				local hrp = plr.Character.HumanoidRootPart
				local humanoid = plr.Character:FindFirstChild("Humanoid")
				
				if humanoid then
					humanoid.PlatformStand = true -- Enable floating
				end
				
				-- Float logic
				local ray = Ray.new(hrp.Position, Vector3.new(0, -10, 0))
				local hit = workspace:FindPartOnRay(ray, plr.Character)
				
				if not hit then
					-- In air, apply upward force
					hrp.Velocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z)
					
					-- Control with keys
					if UIS:IsKeyDown(Enum.KeyCode.Space) then
						hrp.CFrame = hrp.CFrame + Vector3.new(0, 0.5, 0)
					elseif UIS:IsKeyDown(Enum.KeyCode.LeftControl) or UIS:IsKeyDown(Enum.KeyCode.ButtonB) then
						hrp.CFrame = hrp.CFrame + Vector3.new(0, -0.5, 0)
					end
				end
			end
		end)
	end)
end

--=== SWIM IN AIR ===--
local function activateSwim()
	task.spawn(function()
		while states.swim do
			task.wait(0.05)
			if plr.Character then
				local humanoid = plr.Character:FindFirstChild("Humanoid")
				if humanoid then
					humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
					humanoid:SetStateEnabled(Enum.HumanoidStateType.Running, false)
					humanoid:ChangeState(Enum.HumanoidStateType.Swimming)
					
					-- Apply swim movement
					local hrp = plr.Character:FindFirstChild("HumanoidRootPart")
					if hrp then
						-- Control swim direction
						local forward = 0
						local backward = 0
						local left = 0
						local right = 0
						
						if UIS:IsKeyDown(Enum.KeyCode.W) then forward = 1 end
						if UIS:IsKeyDown(Enum.KeyCode.S) then backward = -1 end
						if UIS:IsKeyDown(Enum.KeyCode.A) then left = -1 end
						if UIS:IsKeyDown(Enum.KeyCode.D) then right = 1 end
						
						local moveDirection = (hrp.CFrame.LookVector * (forward + backward)) + 
											  (hrp.CFrame.RightVector * (left + right))
						
						hrp.Velocity = moveDirection * 25 + Vector3.new(0, 5, 0)
					end
				end
			end
		end)
	end)
end

--=== CLICK TELEPORT ===--
local function setupClickTP()
	local connection
	local indicator = Instance.new("Part")
	indicator.Name = "TPIndicator"
	indicator.Size = Vector3.new(2, 0.2, 2)
	indicator.Color = Color3.fromRGB(0, 255, 0)
	indicator.Material = EnumMaterial.Neon
	indicator.Transparency = 0.5
	indicator.Anchored = true
	indicator.CanCollide = false
	indicator.Parent = nil
	
	connection = UIS.InputBegan:Connect(function(input, gameProcessed)
		if not gameProcessed and states.clickTP and input.UserInputType == Enum.UserInputType.MouseButton1 then
			local mouse = plr:GetMouse()
			local target = mouse.Hit.Position
			
			-- Show indicator
			indicator.Position = target + Vector3.new(0, 0.1, 0)
			indicator.Parent = workspace
			
			-- Teleport character
			if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
				plr.Character:FindFirstChild("HumanoidRootPart").CFrame = CFrame.new(target + Vector3.new(0, 5, 0))
				
				-- Visual effect
				local teleportEffect = Instance.new("Part")
				teleportEffect.Size = Vector3.new(5, 5, 5)
				teleportEffect.Position = target
				teleportEffect.Color = Color3.fromRGB(0, 255, 255)
				teleportEffect.Material = EnumMaterial.Neon
				teleportEffect.Transparency = 0.7
				teleportEffect.Anchored = true
				teleportEffect.CanCollide = false
				teleportEffect.Shape = Enum.PartType.Ball
				teleportEffect.Parent = workspace
				
				task.delay(1, function()
					teleportEffect:Destroy()
					indicator.Parent = nil
				end)
			end
		end
	end)
	
	table.insert(connections, connection)
end

--=== INVISIBLE MODE ===--
local function toggleInvisible()
	if not plr.Character then return end
	
	if states.invisible then
		-- Save original transparency and make invisible
		for _, part in pairs(plr.Character:GetDescendants()) do
			if part:IsA("BasePart") then
				originalTransparency[part] = part.Transparency
				part.Transparency = 1
				
				-- Make tool also invisible
				local tool = plr.Character:FindFirstChildOfClass("Tool")
				if tool then
					for _, toolPart in pairs(tool:GetDescendants()) do
						if toolPart:IsA("BasePart") then
							originalTransparency[toolPart] = toolPart.Transparency
							toolPart.Transparency = 1
						end
					end
				end
			end
		end
		
		-- Add shimmer effect
		task.spawn(function()
			while states.invisible do
				task.wait(2)
				if plr.Character then
					for _, part in pairs(plr.Character:GetDescendants()) do
						if part:IsA("BasePart") then
							part.Transparency = 0.2
							task.wait(0.1)
							part.Transparency = 1
						end
					end
				end
			end
		end)
		
	else
		-- Restore original transparency
		for part, transparency in pairs(originalTransparency) do
			if part and part.Parent then
				part.Transparency = transparency
			end
		end
		originalTransparency = {}
	end
end

--=== BUTTON CREATION ===--
local buttons = {}
local yOffset = 5

-- GOD MODE
buttons.god = createButton("üîì God Mode: OFF")
buttons.god.Position = UDim2.new(0.04, 0, 0, yOffset)
yOffset = yOffset + 47

buttons.god.MouseButton1Click:Connect(function()
	states.god = not states.god
	buttons.god.Text = "üîì God Mode: " .. (states.god and "ON" or "OFF")
	
	if states.god then
		task.spawn(function()
			while states.god do
				task.wait(0.2)
				if plr.Character and plr.Character:FindFirstChild("Humanoid") then
					plr.Character.Humanoid.Health = math.huge
				end
			end
		end)
	end
end)

-- EXTRA LIFE (SAFETY)
buttons.extraLife = createButton("üíñ Extra Life: OFF")
buttons.extraLife.Position = UDim2.new(0.04, 0, 0, yOffset)
yOffset = yOffset + 47

buttons.extraLife.MouseButton1Click:Connect(function()
	states.extraLife = not states.extraLife
	buttons.extraLife.Text = "üíñ Extra Life: " .. (states.extraLife and "ON" or "OFF")
	
	if states.extraLife then
		extraLifeUsed = false
		print("[SAFETY] Extra Life activated! Will revive with 1 HP.")
	else
		print("[SAFETY] Extra Life deactivated.")
	end
end)

-- ANTI KILL BLOCK
buttons.antiBlock = createButton("üõ°Ô∏è Anti Kill Block: OFF")
buttons.antiBlock.Position = UDim2.new(0.04, 0, 0, yOffset)
yOffset = yOffset + 47

buttons.antiBlock.MouseButton1Click:Connect(function()
	states.antiBlock = not states.antiBlock
	buttons.antiBlock.Text = "üõ°Ô∏è Anti Kill Block: " .. (states.antiBlock and "ON" or "OFF")
	
	if states.antiBlock then
		connections.block = workspace.DescendantAdded:Connect(function(v)
			if v:IsA("BasePart") then
				v.CanTouch = false
			end
		end)
		
		task.spawn(function()
			for _, v in pairs(workspace:GetDescendants()) do
				if v:IsA("BasePart") then
					v.CanTouch = false
				end
			end
		end)
	else
		if connections.block then
			connections.block:Disconnect()
		end
		task.spawn(function()
			for _, v in pairs(workspace:GetDescendants()) do
				if v:IsA("BasePart") then
					v.CanTouch = true
				end
			end
		end)
	end
end)

-- ANTI NPC
