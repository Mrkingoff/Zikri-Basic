-- ESP PLAYER
buttons.esp = createButton("üëÅÔ∏è ESP Player: OFF")
buttons.esp.Position = UDim2.new(0.04, 0, 0, yOffset)
yOffset = yOffset + 47

buttons.esp.MouseButton1Click:Connect(function()
	states.esp = not states.esp
	buttons.esp.Text = "üëÅÔ∏è ESP Player: " .. (states.esp and "ON" or "OFF")
	
	for _, p in pairs(game:GetService("Players"):GetPlayers()) do
		if p ~= plr and p.Character and p.Character:FindFirstChild("Head") then
			local head = p.Character.Head
			local esp = head:FindFirstChild("ZESP")
			
			if states.esp then
				if not esp then
					local billboard = Instance.new("BillboardGui")
					billboard.Name = "ZESP"
					billboard.Size = UDim2.new(0, 200, 0, 50)
					billboard.AlwaysOnTop = true
					billboard.Adornee = head
					billboard.Parent = head
					
					local label = Instance.new("TextLabel")
					label.Size = UDim2.new(1, 0, 1, 0)
					label.Text = p.Name
					label.TextColor3 = Color3.fromRGB(255, 50, 50)
					label.BackgroundTransparency = 1
					label.Font = Enum.Font.GothamBold
					label.TextSize = 14
					label.Parent = billboard
				end
			else
				if esp then
					esp:Destroy()
				end
			end
		end
	end
end)

-- NOCLIP
buttons.noclip = createButton("üëª Noclip: OFF")
buttons.noclip.Position = UDim2.new(0.04, 0, 0, yOffset)
yOffset = yOffset + 47

buttons.noclip.MouseButton1Click:Connect(function()
	states.noclip = not states.noclip
	buttons.noclip.Text = "üëª Noclip: " .. (states.noclip and "ON" or "OFF")
	
	if states.noclip then
		if connections.noclip then
			connections.noclip:Disconnect()
		end
		connections.noclip = RunService.Stepped:Connect(function()
			if plr.Character then
				for _, part in pairs(plr.Character:GetDescendants()) do
					if part:IsA("BasePart") then
						part.CanCollide = false
					end
				end
			end
		end)
	else
		if connections.noclip then
			connections.noclip:Disconnect()
		end
	end
end)

-- PROMPT NO DURATION
buttons.prompt = createButton("‚è±Ô∏è Instant Prompt: OFF")
buttons.prompt.Position = UDim2.new(0.04, 0, 0, yOffset)
yOffset = yOffset + 47

buttons.prompt.MouseButton1Click:Connect(function()
	states.prompt = not states.prompt
	buttons.prompt.Text = "‚è±Ô∏è Instant Prompt: " .. (states.prompt and "ON" or "OFF")
	
	if states.prompt then
		connections.prompt = workspace.DescendantAdded:Connect(function(obj)
			if obj:IsA("ProximityPrompt") then
				obj.HoldDuration = 0
			end
		end)
		
		task.spawn(function()
			for _, obj in pairs(workspace:GetDescendants()) do
				if obj:IsA("ProximityPrompt") then
					obj.HoldDuration = 0
				end
			end
		end)
	else
		if connections.prompt then
			connections.prompt:Disconnect()
		end
		task.spawn(function()
			for _, obj in pairs(workspace:GetDescendants()) do
				if obj:IsA("ProximityPrompt") then
					obj.HoldDuration = 0.5
				end
			end
		end)
	end
end)

-- IMPROVED FLOAT MODE
